@page
@model PrivacyModel
@{
    ViewData["Title"] = "Claims";
}
<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Type</th>
            <th>ValueType</th>
            <th>Value</th>
        </tr>
    </thead>
    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.ValueType</td>
            <td>@claim.Value</td>
        </tr>
    }
</table>
